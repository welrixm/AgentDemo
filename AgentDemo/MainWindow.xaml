<Window x:Class="AgentDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AgentDemo"
        mc:Ignorable="d"
        Title="Производство продукции" Height="590" Width="990" Icon=".//Resources/Попрыженок.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <Rectangle Fill="#43dcfe">
        </Rectangle>
        <Image HorizontalAlignment="Left" Source="/Resources/Попрыженок.png"></Image>
        <TextBlock Style="{StaticResource TxtBlock1}" Text="Добро пожаловать"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" NavigationUIVisibility="Hidden"></Frame>
        <Rectangle Fill="#43dcfe" Grid.Row="2"></Rectangle>
    </Grid>
</Window>
